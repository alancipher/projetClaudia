{
  "name": "aeemum",
  "private": true,
  "scripts": {
    "start": "meteor --settings settings-development.json",
    "staging": "DEPLOY_HOSTNAME=us-east-1.galaxy-deploy.meteor.com meteor deploy <staging-subdomain>.meteorapp.com --settings settings-staging.json",
    "production": "DEPLOY_HOSTNAME=us-east-1.galaxy-deploy.meteor.com meteor deploy <production-subdomain>.meteorapp.com --settings settings-production.json",
    "test": "jest"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "@cleverbeagle/seeder": "^1.3.1",
    "@material-ui/core": "^1.5.1",
    "@material-ui/icons": "^1.1.1",
    "ajv": "^5.5.2",
    "autosuggest-highlight": "^3.1.1",
    "b64-to-blob": "^1.2.19",
    "babel-plugin-import": "^1.13.3",
    "babel-runtime": "^6.26.0",
    "bcrypt": "^2.0.0",
    "classnames": "^2.3.1",
    "commonmark": "^0.28.1",
    "core-js": "^2.6.12",
    "file-saver": "^2.0.5",
    "graphql": "^0.13.2",
    "handlebars": "^4.7.7",
    "indexof": "0.0.1",
    "jquery": "^2.2.4",
    "jquery-validation": "^1.19.3",
    "jszip": "^3.6.0",
    "juice": "^4.3.2",
    "lodash": "^4.17.21",
    "material-ui": "^0.20.2",
    "meteor-node-stubs": "^0.3.3",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-addons-pure-render-mixin": "^15.6.3",
    "react-autobind": "^1.0.6",
    "react-autosuggest": "^9.4.3",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.14.0",
    "react-flexbox-grid": "^2.1.2",
    "react-markdown": "^3.6.0",
    "react-player": "^1.15.3",
    "react-redux": "^5.1.2",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.3.1",
    "redux": "^4.1.0",
    "redux-api-middleware": "^2.3.0",
    "redux-thunk": "^2.3.0",
    "simpl-schema": "^0.3.2",
    "sweetalert": "^2.1.2",
    "typeface-roboto": "0.0.54",
    "uniforms": "^1.31.1",
    "uniforms--material-ui": "^1.22.10",
    "uniforms-material": "^1.31.1",
    "uniforms-unstyled": "^1.31.1"
  },
  "devDependencies": {
    "babel-jest": "^21.2.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "enzyme": "^3.11.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-meteor": "^4.2.2",
    "eslint-plugin-react": "^7.23.2",
    "jest": "^21.2.1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "meteor",
      "react"
    ],
    "extends": [
      "airbnb",
      "plugin:meteor/recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "browser": true
    },
    "globals": {
      "expect": false
    },
    "rules": {
      "import/no-unresolved": 0,
      "import/no-extraneous-dependencies": 0,
      "import/extensions": 0,
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "components": [
            "Link"
          ],
          "specialLink": [
            "to"
          ]
        }
      ],
      "max-len": 0,
      "no-alert": 0,
      "no-console": 0,
      "no-restricted-globals": 0,
      "no-return-assign": [
        "error",
        "except-parens"
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_id",
            "_ensureIndex",
            "_verifyEmailToken",
            "_resetPasswordToken",
            "_name"
          ]
        }
      ],
      "class-methods-use-this": 0,
      "react/jsx-filename-extension": 0,
      "react/forbid-prop-types": 0
    },
    "settings": {
      "import/resolver": "meteor"
    }
  }
}
